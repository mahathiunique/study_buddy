{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firebase init",
    "indexes": "firestore.rules"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "study_buddy_frontend/storage.rules"
  }
}
